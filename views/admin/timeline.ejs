<% include ../head.ejs %>

<div class="container">
    <div id="visualization"></div>
</div>

<link href="/components/vis/dist/vis-timeline-graph2d.min.css" rel="stylesheet">
<script src="/components/vis/dist/vis-timeline-graph2d.min.js"></script>
<script>
    // DOM element where the Timeline will be attached
    var container = document.getElementById('visualization');

    // Create a DataSet (allows two way data-binding)
    var items = new vis.DataSet([
        {id: 1, title: 'Hello', content: 'item 1', start: '2014-04-20'},
        {id: 2, title: 'Hello', content: 'item 2', start: '2014-04-14'},
        {id: 3, title: 'Hello', content: 'item 3', start: '2014-04-18'},
        {id: 4, title: 'Hello', content: 'item 4', start: '2014-04-16', end: '2014-04-19'},
        {id: 5, title: 'Hello', content: 'item 5', start: '2014-04-25'},
        {id: 6, title: 'Hello', content: 'item 6', start: '2014-04-27', type: 'point'}
    ]);

    // Configuration for the Timeline
    var options = {
//        height: '256px'
    };

    // Create a Timeline
    var timeline = new vis.Timeline(container, items, options);
</script>

